{
   "app_key" : "system.generated.kinectv2process.exe",
   "bindings" : {
      "/actions/calibration" : {
         "sources" : [
            {
               "inputs" : {
                  "position" : {
                     "output" : "/actions/calibration/in/movevertically"
                  }
               },
               "mode" : "trackpad",
               "path" : "/user/hand/right/input/trackpad"
            },
            {
               "inputs" : {
                  "position" : {
                     "output" : "/actions/calibration/in/movehorizontally"
                  }
               },
               "mode" : "trackpad",
               "path" : "/user/hand/left/input/trackpad"
            },
            {
               "inputs" : {
                  "click" : {
                     "output" : "/actions/calibration/in/confirmcalibration"
                  }
               },
               "mode" : "trigger",
               "path" : "/user/hand/right/input/trigger"
            }
         ]
      }
   },
   "controller_type" : "holographic_controller",
   "description" : "",
   "name" : "K2VR Config for WindowsMR - Trackpad Mode"
}
